{
    "name": "innmind/url",
    "type": "library",
    "description": "Library to abstract url handling",
    "keywords": ["url"],
    "homepage": "http://github.com/Innmind/Url",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Url/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/immutable": "~3.0",
        "league/uri": "~5.1"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Url\\": "src/",
            "Fixtures\\Innmind\\Url\\": "fixtures/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Url\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "vimeo/psalm": "~3.7",
        "innmind/black-box": "^4.5"
    },
    "conflict": {
        "innmind/black-box": "<4.0|~5.0"
    },
    "suggest": {
        "innmind/black-box": "For property based testing"
    },
    "provide": {
        "innmind/black-box-sets": "4.0"
    }
}
